@page
@using rikchallengeweb.Pages.Checkout
@using Newtonsoft.Json;
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

@model IndentityModel

<h2>Checkout Page - Identificação</h2>

Pagamento para: @Model._checkoutOrder.client.id <br/>

<form method="post">
    Name: <input asp-for="Name"/> <br/>
    Email: <input asp-for="Email"/> <br/>
    Cpf: <input asp-for="Cpf"/> <br/>
    <input asp-for="CheckoutOrder" />
    <button type="submit">Submit</button>
</form>